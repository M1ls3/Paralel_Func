<Window x:Class="Paralel_Func.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Paralel_Func"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Handle" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Height="43" Width="154" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="44,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Margin="44,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="X:" HorizontalAlignment="Left" Margin="19,6,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Y:" HorizontalAlignment="Left" Margin="19,37,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
